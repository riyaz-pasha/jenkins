pipeline {
    agent any
        environment {
            SAMPLE_CHOICES = ['One', 'Two', 'Three']
        }
    parameters {
        choice(name: 'CHOICE', choices: ${SAMPLE_CHOICES}, description: 'Pick something')
    }
    stages {
        stage('Example') {
            steps {
                echo "Choice: ${params.CHOICE}"
            }
        }
    }
}